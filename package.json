{
  "name": "bitcoin-life-9",
  "version": "1.0.0",
  "type": "module",
  "description": "Bitcoin Life simulation optimized for on-chain deployment with React 18.2.0",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "dev:ord": "node scripts/ord-proxy.js",
    "build": "tsc && vite build",
    "build:ignore-types": "vite build --emptyOutDir",
    "build:eternal": "node scripts/build-for-eternality.js",
    "lint": "powershell -File ./scripts/lint.ps1",
    "lint:fix": "powershell -File ./scripts/lint.ps1 -fix",
    "lint:strict": "powershell -File ./scripts/lint.ps1 -strict",
    "format": "powershell -File ./scripts/format.ps1",
    "format:check": "powershell -File ./scripts/format.ps1 -check",
    "type-check": "tsc --noEmit",
    "start": "vite",
    "preview": "vite preview",
    "clean:mocks": "powershell -File ./scripts/clean-mocks.ps1",
    "test": "npm run clean:mocks && jest",
    "test:watch": "npm run clean:mocks && jest --watch",
    "test:coverage": "npm run clean:mocks && jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "prepare": "husky install",
    "fix:typescript": "cd scripts && npm install && node index.js",
    "fix:typescript:react-imports": "cd scripts && npm install && node fix-react-imports.js",
    "fix:typescript:declarations": "cd scripts && npm install && node create-type-declarations.js",
    "fix:typescript:config": "cd scripts && npm install && node update-tsconfig.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.4.7",
    "@nextui-org/react": "^2.6.11",
    "@react-three/drei": "^9.88.17",
    "@react-three/fiber": "^8.13.7",
    "@types/howler": "^2.2.12",
    "@types/uuid": "^10.0.0",
    "express": "^4.21.2",
    "framer-motion": "^12.5.0",
    "gsap": "^3.12.7",
    "howler": "^2.2.4",
    "postprocessing": "^6.37.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.2.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "three": "^0.162.0",
    "typedi": "^0.10.0",
    "uuid": "^11.1.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@react-three/postprocessing": "^2.15.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.1.2",
    "@types/cypress": "^1.1.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.24",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@types/testing-library__react": "^10.2.0",
    "@types/three": "^0.162.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "cypress": "^13.6.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-check": "^4.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-image-snapshot": "^6.4.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "puppeteer": "^24.7.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.5"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
