{
  "name": "bitcoin-life-9",
  "version": "1.0.0",
  "type": "module",
  "description": "Bitcoin Life simulation optimized for on-chain deployment with React 18.2.0",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "dev:ord": "node scripts/ord-proxy.js",
    "build": "tsc && vite build",
    "build:ignore-types": "vite build --emptyOutDir",
    "build:eternal": "node scripts/build-for-eternality.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src/**/*.ts --fix",
    "start": "vite",
    "preview": "vite preview",
    "fix:typescript": "cd scripts && npm install && node index.js",
    "fix:typescript:react-imports": "cd scripts && npm install && node fix-react-imports.js",
    "fix:typescript:declarations": "cd scripts && npm install && node create-type-declarations.js",
    "fix:typescript:config": "cd scripts && npm install && node update-tsconfig.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.4.7",
    "@nextui-org/react": "^2.6.11",
    "@react-three/drei": "^9.88.17",
    "@react-three/fiber": "^8.13.7",
    "@types/howler": "^2.2.12",
    "@types/uuid": "^10.0.0",
    "express": "^4.21.2",
    "framer-motion": "^12.5.0",
    "gsap": "^3.12.7",
    "howler": "^2.2.4",
    "postprocessing": "^6.37.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.2.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "three": "^0.162.0",
    "typedi": "^0.10.0",
    "uuid": "^11.1.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@react-three/postprocessing": "^2.15.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.24",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@types/testing-library__react": "^10.2.0",
    "@types/three": "^0.162.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.4.2",
    "vite": "^5.1.5"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
